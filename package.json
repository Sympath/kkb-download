{
  "name": "ffmpeg-download",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "formatConfig": "node ./src/formatConfig.js",
    "mail": "node ./src/mail.js",
    "build1": "node ./dist/main.js",
    "build-sh": "sh ./sh/all.sh",
    "build3-debugger": "node ./src/shDownLoad_1.js",
    "build4-linux": "sudo chmod 777 ./ && nohup sh ./download.sh 1>download.log 2>download_err.log &",
    "build4-mac": "nohup sh ./download.sh > download.log 2>&1 &",
    "build4-win": "sh download.sh",
    "retry-linux": "sudo chmod 777 ./ && nohup sh ./retry.sh 1>retry.log 2>retry_err.log &",
    "retry-mac": "nohup sh ./retry-mac.sh > retry.log 2>&1 &",
    "retry-win": "sh ./retry.sh",
    "delete": "node ./dist/delete.js",
    "kill": "node ./dist/kill.js",
    "once": "npm-run-all formatConfig build1 build2 build3 build4 --continue-on-error",
    "once-build": "npm-run-all build1 build2 build3 build4 --continue-on-error",
    "watch": "webpack --watch",
    "build": "webpack",
    "install-linux": "npm install --ignore-scripts puppeteer && npm i && cd node_modules/puppeteer && npm run install && npm run install && cd ../.."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amebyte/ffmpeg-download.git"
